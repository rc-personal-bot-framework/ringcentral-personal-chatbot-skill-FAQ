{
  "name": "ringcentral-personal-chatbot-skill-faq",
  "version": "0.7.0",
  "description": "Bot skill: FAQ, respond to any keywords user defined with corresponding answer",
  "main": "dist/server/index.js",
  "scripts": {
    "ngrok": "ngrok http -region ap 6066",
    "ngrok-p": "ngrok http -region ap 4100",
    "start": "node -r @babel/register ./node_modules/.bin/rcpf build/test-bot.js",
    "c": "./node_modules/.bin/webpack serve --progress --config build/webpack.config.js",
    "compile": "babel src/server --out-dir dist/server",
    "release": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --progress --config build/webpack.config.js",
    "p": "node ./node_modules/.bin/rcpf build/test-bot.js",
    "view": "node build/view.js",
    "build": "npm run clean && npm run compile && npm run release && npm run view",
    "clean": "node build/clean.js",
    "prepublishOnly": "npm run build",
    "i": "npm i && npm i ringcentral-personal-chatbot --no-save"
  },
  "keywords": [
    "RingCentral",
    "Chatbot",
    "AI"
  ],
  "author": "Drake Zhao <drake.zhao@ringcentral.com>",
  "license": "MIT",
  "devDependencies": {
    "@ant-design/icons": "^4.6.2",
    "@babel/cli": "7.14.5",
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-decorators": "7.14.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@babel/register": "7.14.5",
    "antd": "^4.10.3",
    "antd-dayjs-webpack-plugin": "^1.0.6",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-plugin-import": "1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "cross-env": "^5.2.0",
    "css-loader": "5.2.6",
    "css-minimizer-webpack-plugin": "3.0.2",
    "file-loader": "6.2.0",
    "less": "^4.1.1",
    "less-loader": "10.0.1",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "0.9.0",
    "ngrok": "^3.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react-subx": "0.9.0",
    "serverless": "^2.3.0",
    "shelljs": "^0.8.4",
    "standard": "14.3.1",
    "stylus": "^0.54.8",
    "stylus-loader": "6.1.0",
    "subx": "0.9.16",
    "url-loader": "4.1.1",
    "webpack": "5.42.0",
    "webpack-cli": "4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "shortid": "^2.2.14"
  },
  "peerDependencies": {
    "ringcentral-personal-chatbot": ">=0.12.0-beta3"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "/dist/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rc-personal-bot-framework/ringcentral-personal-chatbot-skill-faq.git"
  },
  "bugs": {
